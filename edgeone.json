{
  "name": "edgeone-webhook-pusher",
  "build": {
    "command": "pnpm build",
    "outputDir": ".output/public"
  },
  "devCommand": "pnpm --filter console dev",
  "kv": {
    "namespaces": [
      {
        "binding": "USERS_KV",
        "id": "webhook-pusher-users"
      },
      {
        "binding": "MESSAGES_KV",
        "id": "webhook-pusher-messages"
      },
      {
        "binding": "CHANNELS_KV",
        "id": "webhook-pusher-channels"
      }
    ]
  }
}
